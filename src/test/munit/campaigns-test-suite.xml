		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="sux7cpls-2in9-ica3-9dkd-coa6hdbf8l" actual="#[payload]" expected='#[{
  id: payload.id,
  success: payload.success
}]'/>
		</munit:validation>
