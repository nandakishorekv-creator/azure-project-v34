			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="delete-leads-flow" doc:id="ocsfozyu-nc0n-ytca-if1g-6jijj419vlp" name="delete-leads-flow"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="b472r89l-pup7-3sxy-jh0o-80f1x67nijr" actual="#[payload]" expected='#[{
  id: payload.id,
  success: payload.success
}]'/>
		</munit:validation>
	</munit:test>

</mule>