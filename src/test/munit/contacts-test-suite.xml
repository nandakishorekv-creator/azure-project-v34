	<munit:config name="contacts-test-suite.xml" />
	<munit:test name="post-contacts-flowTest" doc:id="bynrykle-n6b5-lvd6-5mam-rg5opuhw1q" description="Test">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="68hwb6iy-jlnf-uif9-l5c4-kvkk5wdyfcc" processor="salesforce:create">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="create-contact" attributeName="doc:id" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value='#[{
    "id": null,
    "items": [
        {
            "exception": null,
            "message": null,
            "payload": {
                "success": true,
                "id": "001xx000003AAA",
                "errors": []
            },
            "id": "001xx000003AAA",
            "statusCode": null,
            "successful": true
        }
    ],
    "successful": true
}]' />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="post-contacts-flow" doc:id="iysb5gyx-0pyt-onqc-4vgi-gmzcy1vtp5h" name="post-contacts-flow"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="nfpzhbyp-g9ik-rgyh-o6bz-eefim7k8mcw" actual="#[payload]" expected='#[{
  id: payload.id,
  success: payload.success
}]'/>
		</munit:validation>
	</munit:test>
