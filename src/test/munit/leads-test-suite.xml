		http://www.mulesoft.org/schema/mule/munit-tools  http://www.mulesoft.org/schema/mule/munit-tools/current/mule-munit-tools.xsd">
	<munit:config name="leads-test-suite.xml" />
	<munit:test name="post-leads-flowTest" doc:id="bg10ugz4-0k1m-gfu2-0a3h-7ocgzv447wg" description="Test">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="wa16xwa9-98c4-b2vw-2l06-2ty0vf95qyw" processor="salesforce:create">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="create-lead" attributeName="doc:id" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value='#[{
    "id": null,
    "items": [
        {
            "exception": null,
            "message": null,
            "payload": {
                "success": true,
                "id": "001xx000003AAA",
                "errors": []
            },
            "id": "001xx000003AAA",
            "statusCode": null,
            "successful": true
        }
    ],
    "successful": true
}]' />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="post-leads-flow" doc:id="8aedmjjd-n0uz-624i-oq2i-d6dez254fel" name="post-leads-flow"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="m4s9641i-zmbj-daqx-rwp7-tsw2c03gial" actual="#[payload]" expected='#[{
  id: payload.id,
  success: payload.success
}]'/>
		</munit:validation>
	</munit:test>
		<munit:test name="get-leads-flowTest" doc:id="34wju6e0-aq1g-m7kl-0nf2-l1jj4qi0rjp" description="Test">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="75rsiquo-n4zm-11vj-t6ii-y11m4l37elr" processor="salesforce:query">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="query-leads" attributeName="doc:id" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value='#[[
    {
        "Id": "001xx000003AAA",
        "Name": "Test Account",
        "Industry": "Technology"
    }
]]' />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="get-leads-flow" doc:id="e34bcbbh-uhg4-srbf-0jhp-z6czimc9oi" name="get-leads-flow"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="zh7r59tl-yb4a-l1t6-xsod-lj0ighhnzm" actual="#[payload]" expected='#[payload != null]'/>
		</munit:validation>
	</munit:test>
