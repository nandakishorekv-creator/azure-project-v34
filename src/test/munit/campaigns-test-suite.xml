			<munit-tools:mock-when doc:name="Mock when" doc:id="9vqyba6r-m7fi-eazr-5lz0-ovh1bthrj4" processor="salesforce:create">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="create-campaign" attributeName="doc:id" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value='#[{
    "id": null,
    "items": [
        {
            "exception": null,
            "message": null,
            "payload": {
                "success": true,
                "id": "001xx000003AAA",
                "errors": []
            },
            "id": "001xx000003AAA",
            "statusCode": null,
            "successful": true
        }
    ],
    "successful": true
}]' />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="post-campaigns-flow" doc:id="kdvyr1bl-hfqz-rxif-fv67-3q8wd8u19vs" name="post-campaigns-flow"/>
